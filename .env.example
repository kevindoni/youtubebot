# Video Bot Configuration
BOT_MODE=simulation
MAX_VIDEOS_PER_SESSION=5
SESSION_TIMEOUT=3600000

# Anti-Detection Settings
ENABLE_ANTI_DETECTION=true
PROXY_ROTATION_INTERVAL=900000
MAX_REQUESTS_PER_SESSION=50
SESSION_ROTATION_INTERVAL=1800000

# Proxy Settings
PROXY_SOURCES=5
PROXY_VALIDATION_TIMEOUT=10000
PROXY_RETRY_ATTEMPTS=3

# Human Behavior Settings
MIN_WATCH_TIME=10000
MAX_WATCH_TIME=180000
TYPING_SPEED_MIN=50
TYPING_SPEED_MAX=150

# Server Settings
PORT=3000
HOST=localhost

# Logging
LOG_LEVEL=info
LOG_FILE=logs/bot-activity.log

# Comments Configuration
ENABLE_COMMENTS=true
ENABLE_LIKES=true
COMMENT_PROBABILITY=0.3
LIKE_PROBABILITY=0.7
